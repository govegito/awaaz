<script>

$(document).ready(function(){
    $(window).scroll(function(){
        var positiontop=$(document).scrollTop();
        console.log(positiontop);

        if(positiontop > 200){
            $("#img-1").addClass("animated slideInRight");
            

        }
        if(positiontop > 650){
            $("#img-2").addClass("animated slideInLeft");
            

        }
        if(positiontop > 1090){
            $("#ourapp-section1").addClass("animated lightSpeedIn");
            

        }

    });
});
console.log("anime");
</script>